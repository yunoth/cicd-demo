{
  "family": "java-app",
  "networkMode": "awsvpc", 
  "executionRoleArn": "%ECS_TASKEXEC_ARN%",
  "containerDefinitions": [{
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/java-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
    },
    "name": "java-app",
    "essential": true,
    "image": "%ECR_IMAGE%",
    "memoryReservation": 512,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8080,
        "hostPort": 8080
      }
    ]
  }],
  "requiresCompatibilities": [
        "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "1024"
}